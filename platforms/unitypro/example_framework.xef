		<FBProgram name="CHDOFN">
			<STSource>
CMD := CHCFG.CMD;
(*розпаковка з STA*)
VRAW := CHCFG.STA_VRAW;
VAL := CHCFG.STA_VALB;
BAD := CHCFG.STA_BAD;(*керується ззовні*)
PNG := CHCFG.STA_PNG;
ULNK := CHCFG.STA_ULNK;
MERR := CHCFG.STA_MERR;(*керується ззовні*)
