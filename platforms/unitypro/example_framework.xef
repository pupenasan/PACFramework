				</variables>
			</inputParameters>
			<outputParameters>
				<variables name="OUT" typeName="UDINT">
					<comment>Output value</comment>
					<attribute name="PositionPin" value="1"></attribute>
				</variables>
			</outputParameters>
		</ExternalToolsOnly>
	</EFSource>
